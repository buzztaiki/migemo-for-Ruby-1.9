	  trap 'rm -rf elc-lock elc-stamp' 1 2 13 15; \
	  if mkdir elc-lock 2>/dev/null; then \
	    rm -f elc-stamp; \
	    $(MAKE) $(AM_MAKEFLAGS) elc-stamp; \
	    rmdir elc-lock; \
	  else \
	    while test -d elc-lock; do sleep 1; done; \
	    test -f elc-stamp; exit $$?; \
	  fi; \
	else : ; fi
